alias ({"QUIT","quit",})
alias ({"__imud3_chan","$$#1-:write Syntax: $1 <message>.$!;3channel $1 $2*",})
alias ({"abbr","$$#0:write Syntax: abbr <character> [command].$!;&%strlen($1);$$!@=(1):write Syntax: First argument must be one character only.$!;$$#1:quiet alias $1;$$#1:write Restored default abbrevation for $1$!;quiet alias \\\\$1 \\\\\\\\$2 \\$*;write Added abbreviation $1 -> $2",})
alias ({"imud_code","__imud3_chan $0 $*",})
alias ({"imud_gossip","__imud3_chan $0 $*",})
alias ({"laugh","$$@=(down,$*):rofl$!;\\\\laugh $*",})
alias ({"whoami","write \\%N",})
alias ({"ctof","$$#0:write Sntax: ctof <Temperature in Centigrade>$!;&%*(9,$1);&%/(5);write $1 degrees Centigrade is @+(32) degrees Fahrenheit.",})
alias ({"ftoc","$$#0:write Syntax: ftoc <Temperature in Fahrenheit>$!;&%+($1,-32);&%*(5);write $1 degrees Fahrenheit is @/(9) degrees Centigrade.",})
